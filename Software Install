	$checkboxFileZilla = New-Object 'System.Windows.Forms.CheckBox'
	$checkboxChrome = New-Object 'System.Windows.Forms.CheckBox'
	$checkboxAdobe = New-Object 'System.Windows.Forms.CheckBox'
	$InitialFormWindowState = New-Object 'System.Windows.Forms.FormWindowState'
	#endregion Generated Form Objects

	#----------------------------------------------
	# User Generated Script
	#----------------------------------------------
	$buttonInstall_Click = {
		if (-not (Test-Path C:\Temp\)) # Create TEMP folder
			{
				mkdir C:\Temp\
			}
		if ($checkboxAdobe.checked) # Adobe Install
		{
			if (-not (Test-Path 'C:\Program Files (x86)\Adobe\'))
			{
				If (-not (Test-Path C:\Temp\AcroRdrDC1900820071_en_US.exe))
				{
					Invoke-WebRequest 'https://nextcloud.domain.com/index.php/s/EeL32YJLFLYr4ga/download' -OutFile C:\Temp\AcroRdrDC1900820071_en_US.exe
					Start-Process "C:\Temp\AcroRdrDC1900820071_en_US.exe" /sALL -wait
				}
				Else
				{ 
					Start-Process "C:\Temp\AcroRdrDC1900820071_en_US.exe" /sALL -wait
				}
			}
		}
		if ($checkboxChrome.Checked) # Chrome Install
		{
			if (-not (Test-Path 'C:\Program Files (x86)\Google\Chrome\'))
			{
				If (-not (Test-Path C:\Temp\ChromeSetup.exe))
				{
					Invoke-WebRequest 'https://nextcloud.domain.com/index.php/s/MzFCRd2eBQb9EJ5/download' -OutFile C:\Temp\ChromeSetup.exe
					Start-Process "C:\Temp\ChromeSetup.exe" -wait
					Stop-Process -Name "Chrome" -Force
				}
				Else
				{
					Start-Process "C:\Temp\ChromeSetup.exe" -wait
					Stop-Process -Name "Chrome" -Force
				}
			}
		}
		if ($checkboxFileZilla.Checked) # FileZilla Install
		{
			if (-not (Test-Path 'C:\Program Files\FileZilla FTP Client\'))
			{
				If (-not (Test-Path C:\Temp\FileZilla_3.39.0_win64-setup.exe))
				{
					Invoke-WebRequest https://nextcloud.domain.com/index.php/s/CyGTeweLEA8anX5/download -OutFile C:\Temp\FileZilla_3.39.0_win64-setup.exe
					Start-Process "C:\Temp\FileZilla_3.38.1_win64-setup.exe" /quiet -Wait
				}
				Else
				{
					Start-Process "C:\Temp\FileZilla_3.38.1_win64-setup.exe" /quiet -Wait
				}
			}
		}
		if ($checkboxJava.Checked) # Java Install
		{
			Invoke-WebRequest https://nextcloud.domain.com/index.php/s/S5r2axaoNT3qP5N/download -OutFile C:\Temp\JavaSetup8u191.exe
			Start-Process -FilePath C:\Temp\JavaSetup8u191.exe /s
		}
		if ($checkboxOffice365E3.Checked) # Office 365 E3 Install
		{
			mkdir C:\Temp\Office_365_E3\
			Invoke-WebRequest https://nextcloud.domain.com/index.php/s/EALitD2aw7EY8br/download -OutFile 'C:\Temp\Office_365_E3\configuration.xml'
			Invoke-WebRequest https://nextcloud.domain.com/index.php/s/PjXJJapDxjW646n/download -OutFile 'C:\Temp\Office_365_E3\setip.exe'
			Start-Process C:\Temp\Office_365_E3\setup.exe /configure configuration.xml
		}
		if ($checkboxOffice365BusinessPre.Checked) # Office 365 Bussiness Prem Install
		{
			mkdir C:\Temp\Office_365_Busi_Prem\
			Invoke-WebRequest https://nextcloud.domain.com/index.php/s/csLsBxDeYjq3qaP/download -OutFile 'C:\Temp\Office_365_Busi_Prem\configuration.xml'
			Invoke-WebRequest https://nextcloud.domain.com/index.php/s/RkEpX6yrZZHmTTB/download -OutFile 'C:\Temp\Office_365_Busi_Prem\setup.exe '
			Start-Process C:\Temp\Office_365_Busi_Prem\setup.exe /configure configuration.xml
		}
		if ($checkboxOffice2016.Checked) # Office 2016 Install
		{
			Start-Process "\\PUREPRINT-FILE\Client Apps\MS Office\Office 2016\Win VL\Office (2016) 32Bit\setup.exe" -Wait
			cscript 'C:\Program Files (x86)\Microsoft Office\Office16\OSPP.VBS' /inpkey:$Office2016Key.Text
			cscript 'C:\Program Files (x86)\Microsoft Office\Office16\OSPP.VBS' /act
		}
		if ($checkboxCNCSTD.Checked) # Install CNC Defauts
		{ Invoke-Command -ScriptBlock {
				net start w32time
				POWERCFG -Change -monitor-timeout-ac 10
				POWERCFG -CHANGE -monitor-timeout-dc 5
				POWERCFG -CHANGE -disk-timeout-ac 30
				POWERCFG -CHANGE -disk-timeout-dc 30
				POWERCFG -CHANGE -standby-timeout-ac 0
				POWERCFG -CHANGE -standby-timeout-dc 60
				POWERCFG -CHANGE -hibernate-timeout-ac 0
				POWERCFG -CHANGE -hibernate-timeout-dc 0
				powercfg -setacvalueindex SCHEME_CURRENT 4f971e89-eebd-4455-a8de-9e59040e7347 7648efa3-dd9c-4e3e-b566-50f929386280 3
				powercfg -setdcvalueindex SCHEME_CURRENT 4f971e89-eebd-4455-a8de-9e59040e7347 7648efa3-dd9c-4e3e-b566-50f929386280 3
			}
		}
		if ($checkboxLabtechGeneric.Checked) # Install Labtech Defaults
		{
			if (-not (Test-Path C:\Windows\LTSvc\))
			{
				Invoke-WebRequest https://nextcloud.domain.com/index.php/s/qHmqDm4Sdpa2QKN/download -OutFile C:\temp\Labtech-Generic.exe
				Start-Process C:\temp\Labtech-Generic.exe /passive -Wait		
			}
		}
		if ($checkboxOfficeUninstaller.Checked) # Uninstall Office
		{
			
			Invoke-WebRequest https://nextcloud.domain.com/index.php/s/gDF9ofAqEF3P3Mo/download -OutFile C:\Temp\Office_Removal_Tool.diagcab
			Start-Process C:\Temp\Office_Removal_Tool.diagcab
		}
		if ($checkboxUKLanuagePack.Checked) # Install GB Lanuage Pack
		{
			Invoke-WebRequest https://nextcloud.domain.com/index.php/s/qj9zHZJsMXNX3nA/download -OutFile C:\temp\UKLangPack.cab
			lpksetup /i /p C:\temp\UKLangPack.cab
			lpksetup /u es-US
			Set-WinSystemLocale en-GB
			Set-WinUserLanguageList en-GB
		}
		if ($checkboxDellCommand.checked) # Install Dell Command
		{
			Invoke-WebRequest https://nextcloud.domain.com/index.php/s/Sr6DQxmQJZXYH5S/download -OutFile C:\Temp\DellCommand.exe
			Start-Process C:\Temp\DellCommand.exe /s
		}
		if ($checkboxSelectAll.Checked)
		{
			Check
			
			
		}
		
	}
	
	#
	$buttonCancel_Click={
		$formInstallSoftware.Close()
		}
