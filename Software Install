	$formInstallSoftware = New-Object 'System.Windows.Forms.Form'
	$checkboxOfficeUninstaller = New-Object 'System.Windows.Forms.CheckBox'
	$checkboxUKLanuagePack = New-Object 'System.Windows.Forms.CheckBox'
	$Office2016Key = New-Object 'System.Windows.Forms.TextBox'
	$checkboxDellCommand = New-Object 'System.Windows.Forms.CheckBox'
	$checkboxJava = New-Object 'System.Windows.Forms.CheckBox'
